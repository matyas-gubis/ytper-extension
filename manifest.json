{
  "manifest_version": 3,
  "name": "Ytper Solver",
  "description": "An extension for the game Ytper (ytper.com), that can map the shuffled keyboard , automatically solve the puzzle and even more.",
  "action": {
    "default_popup": "src/pages/popup/index.html",
    "default_icon": {
      "32": "icon-32.png"
    }
  },
  "icons": {
    "128": "icon-128.png"
  },
  "permissions": [
    "activeTab",
    "scripting",
    "storage",
    "sidepanel"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://ytper/*"
      ],
      "js": [
        "src/pages/content/index.tsx"
      ],
      "css": [
        "contentStyle.css"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "contentStyle.css",
        "icon-128.png",
        "icon-32.png"
      ],
      "matches": []
    }
  ]
}