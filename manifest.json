{
  "manifest_version": 3,
  "name": "Ypter key mapper",
  "version": "1.0",
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "scripting",
    "activeTab"
  ],
  "content_scripts": [
    {
      "css": [
        "style/style.css"
      ],
      "js": [
        "scripts/pako.min.js",
        "scripts/socket.io.min.js",
        "scripts/content.js"
      ],
      "matches": [
        "https://ytper.com/*"
      ]
    }
  ]
}